print("Enter three values:")
no1=float(input())
no2=float(input())
no3=float(input())

#compare three values
if no1>no2 and no1>no3:
    print("max value is:",no1)
elif no2>no3:
    print("max value is:",no2)
else:
    print("max value is:",no3)

